CREATE TABLE sale (
  upc TEXT NOT NULL ,
  chequeNumber TEXT NOT NULL,
  productNumber INTEGER NOT NULL,
  sellingPrice REAL NOT NULL,
  PRIMARY KEY (upc, chequeNumber)
);

