getSaleByUpcAndChequeNumber:
SELECT *
FROM sale
WHERE upc = :cardNumber AND chequeNumber = :chequeNumber;

getAllSales:
SELECT *
FROM sale;

getSalesWithChequeNumber:
SELECT *
FROM sale
WHERE chequeNumber = ?;

insertSale:
INSERT OR REPLACE
INTO sale
VALUES(?, ?, ?, ?);

deleteSaleByUpcAndChequeNumber:
DELETE FROM sale
WHERE upc = :cardNumber AND chequeNumber = :chequeNumber;