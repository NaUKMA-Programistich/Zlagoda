getCustomerCardByCardNumber:
SELECT *
FROM customerCard
WHERE cardNumber = :cardNumber;

getAllCustomerCards:
SELECT *
FROM customerCard
ORDER BY custSurname;

insertCustomerCard:
INSERT OR REPLACE
INTO customerCard
VALUES(?, ?, ?, ?, ?, ?, ?, ?, ?);

deleteCustomerCardByCardNumber:
DELETE FROM customerCard
WHERE cardNumber = :cardNumber;

searchBySurname:
SELECT *
FROM customerCard
WHERE custSurname LIKE '%' || ? || '%';

searchByPercent:
SELECT *
FROM customerCard
WHERE percent = ?
ORDER BY custSurname;