CREATE TABLE customerCard (
  cardNumber TEXT NOT NULL PRIMARY KEY,
  custSurname TEXT NOT NULL,
  custName TEXT NOT NULL,
  custPatronymic TEXT,
  phoneNumber TEXT NOT NULL,
  city TEXT,
  street TEXT,
  zipCode TEXT,
  percent INTEGER NOT NULL
);

